    "postcss": "8.4.16",
    "postcss-import": "14.1.0",
    "prettier": "2.7.1",
    "terser": "5.14.2",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.11.1",
    "unplugin-vue-components": "0.22.4",
    "vite": "3.0.6",
    "vite-plugin-eslint": "1.8.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vue-tsc": "0.40.1"
  },
  "engines": {
    "node": ">16.0.0"
  }
